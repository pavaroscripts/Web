@charset "utf-8";

* { margin: 0px; padding: 0px; box-sizing: border-box; }

body { font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif; background: linear-gradient(135deg, rgb(15, 12, 41) 0%, rgb(48, 43, 99) 50%, rgb(36, 36, 62) 100%); color: rgb(255, 255, 255); min-height: 100vh; overflow-x: hidden; }

.login-container { display: flex; justify-content: center; align-items: center; min-height: 100vh; background: linear-gradient(135deg, rgb(15, 12, 41) 0%, rgb(48, 43, 99) 50%, rgb(36, 36, 62) 100%); }

.login-box { background: rgba(255, 255, 255, 0.05); border: 1px solid rgba(255, 255, 255, 0.1); padding: 40px; border-radius: 15px; backdrop-filter: blur(10px); width: 100%; max-width: 400px; box-shadow: rgba(0, 0, 0, 0.3) 0px 8px 32px; }

.login-box h1 { text-align: center; margin-bottom: 10px; font-size: 2.5em; background: linear-gradient(45deg, rgb(0, 212, 255), rgb(124, 58, 237)) text; -webkit-text-fill-color: transparent; }

.login-box p { text-align: center; color: rgba(255, 255, 255, 0.7); margin-bottom: 30px; font-size: 0.9em; }

.input-group { margin-bottom: 20px; }

.input-group label { display: block; margin-bottom: 8px; color: rgba(255, 255, 255, 0.9); font-weight: 500; }

.input-group input { width: 100%; padding: 12px; border: 1px solid rgba(255, 255, 255, 0.2); background: rgba(255, 255, 255, 0.05); color: rgb(255, 255, 255); border-radius: 8px; font-size: 1em; transition: 0.3s; }

.input-group input:focus { outline: none; border-color: rgb(0, 212, 255); background: rgba(0, 212, 255, 0.05); }

.login-btn { width: 100%; padding: 12px; background: linear-gradient(45deg, rgb(0, 212, 255), rgb(124, 58, 237)); border: none; color: rgb(255, 255, 255); font-weight: bold; border-radius: 8px; cursor: pointer; font-size: 1em; transition: 0.3s; margin-bottom: 15px; }

.login-btn:hover { transform: translateY(-2px); box-shadow: rgba(0, 212, 255, 0.4) 0px 5px 20px; }

.login-btn:active { transform: translateY(0px); }

.error { background: rgba(255, 59, 48, 0.2); border: 1px solid rgb(255, 59, 48); color: rgb(255, 107, 107); padding: 12px; border-radius: 8px; margin-bottom: 15px; display: none; text-align: center; }

.error.show { display: block; }

.discord-link { text-align: center; margin-top: 20px; }

.discord-link a { color: rgb(88, 101, 242); text-decoration: none; font-weight: 500; transition: 0.3s; }

.discord-link a:hover { text-decoration: underline; }

.main-container { display: none; min-height: 100vh; padding: 20px; }

.main-container.active { display: block; }

.header { text-align: center; margin-bottom: 40px; padding-bottom: 20px; border-bottom: 1px solid rgba(255, 255, 255, 0.1); }

.header h1 { font-size: 3em; background: linear-gradient(45deg, rgb(0, 212, 255), rgb(124, 58, 237)) text; -webkit-text-fill-color: transparent; margin-bottom: 10px; }

.header p { color: rgba(255, 255, 255, 0.7); margin-bottom: 15px; }

.header-buttons { display: flex; gap: 10px; justify-content: center; flex-wrap: wrap; }

.header-buttons button { padding: 10px 20px; background: rgba(255, 255, 255, 0.1); border: 1px solid rgba(255, 255, 255, 0.2); color: rgb(255, 255, 255); border-radius: 8px; cursor: pointer; transition: 0.3s; }

.header-buttons button:hover { background: rgba(0, 212, 255, 0.2); border-color: rgb(0, 212, 255); }

.scripts-container { display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 20px; max-width: 1200px; margin: 0px auto; }

.script-card { background: rgba(255, 255, 255, 0.05); border: 1px solid rgba(255, 255, 255, 0.1); padding: 25px; border-radius: 12px; backdrop-filter: blur(10px); transition: 0.3s; cursor: pointer; }

.script-card:hover { transform: translateY(-5px); border-color: rgb(0, 212, 255); background: rgba(0, 212, 255, 0.05); box-shadow: rgba(0, 212, 255, 0.2) 0px 8px 32px; }

.script-card h3 { color: rgb(0, 212, 255); margin-bottom: 10px; font-size: 1.3em; }

.script-card p { color: rgba(255, 255, 255, 0.7); margin-bottom: 15px; font-size: 0.95em; }

.script-status { display: inline-block; padding: 5px 12px; background: rgba(76, 175, 80, 0.2); border: 1px solid rgb(76, 175, 80); color: rgb(76, 175, 80); border-radius: 5px; font-size: 0.85em; margin-bottom: 15px; }

.script-status.new { background: rgba(255, 193, 7, 0.2); border-color: rgb(255, 193, 7); color: rgb(255, 193, 7); }

.button-group { display: flex; gap: 10px; }

.copy-btn, .execute-btn { flex: 1 1 0%; padding: 10px; border: none; border-radius: 8px; cursor: pointer; font-weight: 500; transition: 0.3s; font-size: 0.9em; }

.copy-btn { background: rgba(100, 200, 255, 0.3); color: rgb(100, 200, 255); border: 1px solid rgb(100, 200, 255); }

.copy-btn:hover { background: rgba(100, 200, 255, 0.5); }

.execute-btn { background: linear-gradient(45deg, rgb(0, 212, 255), rgb(124, 58, 237)); color: rgb(255, 255, 255); }

.execute-btn:hover { transform: scale(1.05); box-shadow: rgba(0, 212, 255, 0.4) 0px 5px 20px; }

.notification { position: fixed; top: 20px; right: 20px; background: rgba(76, 175, 80, 0.9); color: rgb(255, 255, 255); padding: 15px 25px; border-radius: 8px; display: none; z-index: 1000; animation: 0.3s ease 0s 1 normal none running slideIn; }

.notification.show { display: block; }

@keyframes slideIn { 
  0% { transform: translateX(400px); opacity: 0; }
  100% { transform: translateX(0px); opacity: 1; }
}

.logout-btn { position: fixed; top: 20px; left: 20px; padding: 10px 20px; background: rgba(255, 59, 48, 0.3); border: 1px solid rgb(255, 59, 48); color: rgb(255, 107, 107); border-radius: 8px; cursor: pointer; transition: 0.3s; z-index: 100; }

.logout-btn:hover { background: rgba(255, 59, 48, 0.5); }

@media (max-width: 768px) {
  .login-box { margin: 20px; }
  .header h1 { font-size: 2em; }
  .scripts-container { grid-template-columns: 1fr; }
}
